    "start:api": "nx serve api",
    "build": "pnpm run config && nx build web",
    "migrate": "nx run api:migrate",
    "test": "nx run-many --target=test --exclude='api' --parallel",
    "e2e:tests": "nx run test api",
    "start:docker": "docker compose --env-file=.env up -d",
    "stop:docker": "docker compose --env-file=.env down",
    "start:docker:clean": "docker compose --env-file=.env up -d --force-recreate --build",
    "regenerate": "nx run api:regenerate",
    "generate": "drizzle-kit generate:pg",
    "studio": "pnpm exec drizzle-kit studio",
    "lint": "nx run-many --all --target=lint --parallel --verbose",
    "format": "nx format:write --libs-and-apps",
    "demo": "nx run api:demo",
    "prepare": "husky",
    "serve-dist": "nx run api:serveDist"
