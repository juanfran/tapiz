{
  "name": "team-up",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "config": "node --env-file=.env scripts/setup-env.js",
    "ng": "nx",
    "start": "npm run config && nx serve team-up --port 4300",
    "start:api": "nx serve api",
    "build": "npm run config && nx build team-up",
    "migrate": "nx run api:migrate",
    "test": "nx run-many --target=test --exclude='api'",
    "e2e:tests": "nx run test api",
    "start:docker": "cd docker && docker compose --env-file=../.env up -d",
    "stop:docker": "cd docker && docker compose --env-file=../.env down",
    "start:docker:clean": "cd docker && docker compose --env-file=../.env up -d --force-recreate --build",
    "regenerate": "nx run api:regenerate",
    "generate": "drizzle-kit generate:pg",
    "studio": "npx drizzle-kit studio",
    "lint": "nx run-many --all --target=lint --parallel",
    "format": "nx format:write --libs-and-apps",
    "prepare": "husky"
  },
  "private": true,
